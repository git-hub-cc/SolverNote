{
  "name": "solvernote",
  "version": "2.0.1",
  "description": "AI-Assisted Knowledge Management Flow",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"npm run dev:ui\" \"npm run dev:electron\"",
    "dev:ui": "vite",
    "dev:electron": "wait-on tcp:5173 && electron .",
    "build": "vite build && electron-builder",
    "preview": "vite preview"
  },
  "dependencies": {
    "@lancedb/lancedb": "^0.22.3",
    "chokidar": "^3.6.0",
    "dayjs": "^1.11.10",
    "electron-fetch": "^1.9.1",
    "electron-log": "^5.4.3",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "lucide-vue-next": "^0.555.0",
    "markdown-it": "^14.1.0",
    "node-llama-cpp": "^3.12.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "concurrently": "^8.2.2",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "sass": "^1.70.0",
    "vite": "^5.0.12",
    "wait-on": "^7.2.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "ppmc.club.SolverNote",
    "productName": "SolverNote",
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "asarUnpack": [
      "**/node_modules/node-llama-cpp/**/*",
      "**/node_modules/@lancedb/lancedb/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "src/assets/icon/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "src/assets/icon/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "src/assets/icon/icon.png"
    }
  }
}
